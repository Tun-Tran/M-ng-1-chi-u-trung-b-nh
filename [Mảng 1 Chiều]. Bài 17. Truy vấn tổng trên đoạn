#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
   int n;cin>>n;
   long long a[n];
    for(int i=0;i<n;i++){
        cin>>a[i];
    }
    int k;cin>>k;
    long long s=0;
    for(int i=1;i<=k;i++){
        int l, r;cin>>l>>r;
        for(int j=l-1;j<r;j++){
            s+=a[j];
            
        }
        cout<<s<<endl;
        s=0;
    }
   
    return 0;
}
